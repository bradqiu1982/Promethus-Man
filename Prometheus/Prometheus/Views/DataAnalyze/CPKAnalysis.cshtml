
@{
    ViewBag.Title = "CPK Analysis";
    Layout = "~/Views/Shared/_LayoutNew.cshtml";
}

<link type="text/css" rel="stylesheet" href="~/Scripts/bootstrap-tagsinput/bootstrap-tagsinput.css" />
<script type="text/javascript" src="~/Scripts/bootstrap-tagsinput/bootstrap-tagsinput.js"></script>
<script type="text/javascript" src="~/Scripts/bootstrap-tagsinput/bootstrap3-typeahead.js"></script>
<script type="text/javascript" src="~/Scripts/cpkanalysis.js"></script>

<script type="text/javascript">
    $(function () {
        CPKANALYSE.init();
    })
</script>

<style type="text/css">
    .bootstrap-tagsinput {
        width:100%!important;    
    }
</style>

<div class="row" style="margin-top:2%">
    <ol class="breadcrumb" style="width:600px">
        <li>Data Analysis</li>
        <li class="active">CPK Analysis</li>
    </ol>
</div>

<div class="row">
    <div class="col-xs-2" MYTP="orange" style="font-family:'Arial Narrow';font-size:18px"><strong>&nbsp;&nbsp;Query</strong></div>
    <div class="col-xs-10">

        <div class="form-group">
            <div class="row">
                <div class="col-xs-6">
                    <label class="control-label col-xs-2" >Project<font color="red">*</font></label>
                    <div class="col-xs-10">
                        <input class="form-control" id="projectlist" name="projectlist" type="text" value=""/>
                    </div>
                </div>
                <div class="col-xs-6">
                    <label class="control-label col-xs-2" >Data Table<font color="red">*</font></label>
                    <div class="col-xs-10">
                        <input class="form-control" id="mestablelist" name="mestablelist" type="text" value="" readonly />
                    </div>
                </div>
            </div>
        </div>

        <div class="form-group">
            <div class="row">
                <div class="col-xs-6">
                    <label class="control-label col-xs-2">PN DES/PN<font color="red">*</font></label>
                    <div class="col-xs-10">
                        <input class="form-control" id="pnlist" name="pnlist"  data-provide="typeahead" autocomplete="off" type="text" value="" readonly />
                    </div>
                </div>
                <div class="col-xs-6">
                    <label class="control-label col-xs-2">Parameter<font color="red">*</font></label>
                    <div class="col-xs-10">
                        <input class="form-control" id="queryparam" name="queryparam" type="text" value="" readonly />
                    </div>
                </div>
            </div>
        </div>

        <div class="form-group">
            <div class="row">
                <div class="col-xs-6">
                    <label class="control-label col-xs-2">Low Limit</label>
                    <div class="col-xs-10">
                        <input class="form-control text-box single-line" id="lowlimit" name="lowlimit" type="text" value="" readonly />
                    </div>
                </div>
                <div class="col-xs-6">
                    <label class="control-label col-xs-2">High Limit</label>
                    <div class="col-xs-10">
                        <input class="form-control text-box single-line" id="highlimit" name="highlimit" type="text" value="" readonly />
                    </div>
                </div>
            </div>
        </div>

        <div class="form-group">
            <div class="row">
                <div class="col-xs-6">
                    <label class="control-label col-xs-2">Start<font color="red">*</font></label>
                    <div class="col-xs-10">
                        <input class="form-control text-box single-line" id="StartDate" placeholder="Start Date" name="StartDate" type="text" value="" />
                    </div>
                </div>
                <div class="col-xs-6">
                    <label class="control-label col-xs-2">End<font color="red">*</font></label>
                    <div class="col-xs-10">
                        <input class="form-control text-box single-line" id="EndDate" placeholder="End Date" name="EndDate" type="text" value="" />
                    </div>
                </div>
            </div>
        </div>

        <div class="form-group">
            <div class="row">
                <div class="col-xs-6">
                    <label class="control-label col-xs-2">Only PASS</label>
                    <div class="col-xs-10">
                        @Html.DropDownList("allpasslist", null, new { @id = "allpasslist", @class = "form-control" })
                    </div>
                </div>
                <div class="col-xs-6">
                    <label class="control-label col-xs-2">Database</label>
                    <div class="col-xs-7">
                        @Html.DropDownList("databaselist", null, new { @id = "databaselist", @class = "form-control" })
                    </div>
                    <div class="col-xs-3">
                        <button class="btn btn-primary" id="QueryCPK" name="QueryCPK">Get CPK</button>
                    </div>
               </div>
            </div>
        </div>
    </div>
</div>

<hr style="border-color:#006DC0;" />

<div class="row">
    <div class="col-xs-2" MYTP="green" style="font-family:'Arial Narrow';font-size:18px"><strong>&nbsp;&nbsp;Output</strong></div>
    <div class="col-xs-10">
        <div class="form-group">
            <div class="row">
                <div class="col-xs-6">
                    <label class="control-label col-xs-4" >Mean</label>
                    <div class="col-xs-8">
                        
                    </div>
                </div>
                <div class="col-xs-6">
                    <label class="control-label col-xs-4" >StdDev</label>
                    <div class="col-xs-8">
                        
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-xs-6">
                    <label class="control-label col-xs-4">Normal Probability</label>
                    <div class="col-xs-8">

                    </div>
                </div>
                <div class="col-xs-6">
                    <label class="control-label col-xs-4">General CPK</label>
                    <div class="col-xs-8">

                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-xs-6">
                    <label class="control-label col-xs-4">Robust CPK</label>
                    <div class="col-xs-8">

                    </div>
                </div>
                <div class="col-xs-6">
                    <label class="control-label col-xs-4">Robust DPPM</label>
                    <div class="col-xs-8">

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<hr style="border-color:#006DC0;" />

<div class="row">
    <div class="col-xs-2" MYTP="red" style="font-family:'Arial Narrow';font-size:18px"><strong>&nbsp;&nbsp;Chart</strong></div>
    <div class="col-xs-10">
        <div class="form-group">
            <div class="row">
                
            </div>
        </div>
    </div>
</div>

