@model IEnumerable<Prometheus.Models.UserCacheVM>

@{
    ViewBag.Title = "UserCachedInfo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>User Cached Info</h2>

<hr/>

@if (Model != null)
{
    foreach (var item in Model)
    {
        <div class="form-group">
            <div class="col-xs-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="col-xs-5">
                                    <h3 class="panel-title">@item.UpdateTime.ToString()</h3>
                                </div>
                                <div class="col-xs-7"></div>
                            </div>
                        </div>
                    </div>
                    <div class="panel-body" style="overflow-y: scroll;overflow-x: scroll;">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-xs-1"></div>
                                <div class="col-xs-10">@Html.Raw(item.CacheInfo)</div>
                                <div class="col-xs-1"></div>
                            </div>
                        </div>
                   </div>
                </div>
            </div>
        </div>
        <hr/>
    }
}

