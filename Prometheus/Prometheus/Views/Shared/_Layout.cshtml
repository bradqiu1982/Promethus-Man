        <script type="text/javascript">
            $(window).load(function () {
                $(".se-pre-con").fadeOut("slow");
            });

                        function RealSaveUserContent(myval)
                        {
                            if (myval)
                            {
                                var userdata = {};
                                $.base64.utf8encode = true;
                                userdata['myval'] = $.base64.btoa(myval);

                                $.ajax({
                                    url: "/User/SaveCacheInfo",
                                    data: userdata,
                                    type: 'POST',
                                    dataType: 'text',
                                    success: function (data, e)
                                    {
                                        alert(data);
                                    },
                                    error: function (e)
                                    {
                                        alert(e);
                                    }
                                });
                            }

                            return false;
                        }

                        function UserCacheContent()
                        {
                            var myurl = '/User/UserCachedInfo';
                            window.open(myurl, '_blank');
                        }

                        function npireadmode()
                        {
                            $('.form-control').attr('disabled', 'disabled');
                            $('input').attr('disabled', 'disabled');
                        }

                        function npieditmode()
                        {
                            $('.form-control').removeAttr('disabled');
                            $('input').removeAttr('disabled');
                        }

                        function changepagemode()
                        {
                            if (document.getElementById("editablebt").innerText == 'Edit Mode') {
                                document.getElementById("editablebt").innerText = 'Review Mode';
                                npieditmode();
                            }
                            else
                            {
                                document.getElementById("editablebt").innerText = 'Edit Mode';
                                 npireadmode();
                            }
                        }
        </script>
