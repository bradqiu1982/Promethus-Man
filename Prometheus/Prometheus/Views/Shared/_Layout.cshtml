            border-bottom-right-radius: 1.5em;
            height: 3em;
            background-color: #006DC0;
            color: white;
        }

        input[type="text"] {
            border: 1px;
            border-style: solid;
            border-color: #006DC0;
            font-family: 'Arial Narrow';
        }

        textarea.form-control {
            border: 1px;
            border-style: solid;
            border-color: #006DC0;
            font-family: 'Arial Narrow';
            overflow-y: auto !important;
            overflow-x: auto !important;
        }

        select.form-control {
            border: 1px;
            border-style: solid;
            border-color: #006DC0;
            font-family: 'Arial Narrow';
        }

        thead {
            background-color: #006DC0;
            font-family: 'Arial Narrow';
