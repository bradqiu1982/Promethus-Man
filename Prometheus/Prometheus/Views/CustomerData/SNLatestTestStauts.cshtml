
@{
    ViewBag.Title = "Module Latest Test Stauts";
    Layout = "~/Views/Shared/_LayoutNew.cshtml";
}

<link type="text/css" rel="stylesheet" href="~/Content/snlatestteststatus.css" />
<script type="text/javascript" src="~/Scripts/jquery-bootstrap-loading.js"></script>
<script type="text/javascript" src="~/Scripts/snlatestteststatus.js"></script>
<script type="text/javascript">
    $(function(){
        SNSTATUS.INIT();
    })
</script>

<div class="row" style="border-color:gray;height:90%;padding:5px;">
<div class="col-xs-4 mark-lists">
    <div class="marks-title">
        <span>Please Scan SN:</span>
    </div>
    <textarea class="form-control marks" id="marks"></textarea>
    <div class="marks-submit">
        <div class="total-marks label-danger col-xs-2" id="total-marks">0</div>
        <div class="col-xs-3">
            <input type="button" class="btn btn-warning btn-marks-clean" id="btn-marks-clean" value="清除">
        </div>
        <div class="marks-op col-xs-4 ">
            <input type="button" class="btn btn-success btn-marks-OQM" id="btn-marks-OQM" value="OQM">
        </div>
        <div class="marks-op col-xs-3 ">
            <input type="button" class="btn btn-success btn-marks-submit" id="btn-marks-submit" value="查询">
        </div>
    </div>
</div>
<div class="col-xs-8 mark-detail">
    <div class="mark-detail-op">
        <span class="glyphicon glyphicon-download-alt op-download"></span>
    </div>
    <table class="table  table-condensed" id="mywafertable">
        <caption class="tb-caption">SN STATUS</caption>
        <thead>
            <tr>
                <th>SN</th>
                <th>Which Test</th>
                <th>Failure</th>
                <th>Test Time</th>
                <th>Test Station</th>
                <th>PN</th>
                <th>CSN</th>
                <th>Module Type</th>
                
            </tr>
        </thead>
        <tbody id="WaferTableID"></tbody>
    </table>
</div>
</div>