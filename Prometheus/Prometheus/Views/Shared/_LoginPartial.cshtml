@using Prometheus.Models;


    <ul class="nav navbar-nav div-nav-right">
        @{
            var ckdict = CookieUtility.UnpackCookie(Request);
            if (ckdict.ContainsKey("logonuser") && !string.IsNullOrEmpty(ckdict["logonuser"]))
            {
                <li>
                    <a href="/User/LoginOutUser">
                        <span class="glyphicon glyphicon-log-out" title="Log Out" data-toggle="tooltip"></span>
                    </a>
                </li>
            }
            else
            {
                <li>
                    <a href="/User/LoginUser">
                        <span class="glyphicon glyphicon-log-in" title="Log In" data-toggle="tooltip"></span>
                    </a>
                </li>
            }
        }
        
    </ul>



