
@{
    ViewBag.Title = "ProjectYield";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/YieldMain.js"></script>

<div class="row">
    <div class="col-xs-1 text-right">
        <img src="~/Content/images/PJ/Yield.png" style="height:45px; width:45px">
    </div>
    <div class="col-xs-11">
        <ol class="breadcrumb" style="font-size:18px">
            <li><a data-toggle="tooltip" title="Goto ViewAll" href="/Project/ViewAll">All Projects</a></li>
            @if (ViewBag.pjkey != null)
            {
                <li><a data-toggle="tooltip" title="Goto Project Detail" href="/Project/ProjectDetail?ProjectKey=@(ViewBag.pjkey)">Project Detail</a></li>
            }

            @if (ViewBag.pjkey != null)
            {
                <li><a data-toggle="tooltip" title="Goto Yield Main Page" href="/Project/ProjectYieldMain?ProjectKey=@(ViewBag.pjkey)">Yield Main</a></li>
            }

            @if (ViewBag.pjkey != null)
            {
                <li class="active">@ViewBag.pjkey Yield</li>
            }
            else
            {
                <li class="active">Project Yield</li>
            }
        </ol>
    </div>
</div>

<div class="row">
    <div class="col-xs-12">&nbsp;</div>
</div>

<form>
    @if (ViewBag.pjkey != null)
    {
        <input  value="@(ViewBag.pjkey)" id="pjkey" name="pjkey" type="hidden" />
    }
    @if (ViewBag.wks != null)
    {
        <input value="@(ViewBag.wks)" id="wks" name="wks" type="hidden" />
    }
</form>

@if (ViewBag.chartscript != null)
{
    <script type="text/javascript">
    $(function () {
        YIELDMAIN.errorinit();
    })
    </script>

    <div class="row">
                        <div id="weeklyyield" style="min-width: 310px; height: 400px; margin: 0 auto"></div>
    </div>
    @Html.Raw(ViewBag.chartscript)

    <div class="row">
        <div id="errorid" style="min-width: 310px; height: 400px; margin: 0 auto"></div>
    </div>
}

@if (ViewBag.brchartscript != null)
{

    <div class="row">
            <div id="bryield" style="min-width: 310px; height: 400px; margin: 0 auto"></div>
    </div>
    @Html.Raw(ViewBag.brchartscript)
}